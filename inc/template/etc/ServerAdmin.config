<configuration>
  <appSettings>
    <add key="Server.PathTemp" value="${dir.root}/tmp" />
    <add key="ServerAdmin.LogFile" value="${dir.root}/log/ServerAdmin.log" />
    <add key="NumberOfLogFilesToKeep" value="6" />                                      <!-- Default Value: "-1" -->
    <add key="OpenPetra.HTTPServer" value="http://127.0.0.1:${Server.Port}/api/"/>
    <add key="HTTPUtils.PostRequests" value="1" />
  </appSettings>
  
</configuration>
