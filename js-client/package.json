{
  "name": "openpetra-client-js",
  "version": "2018.2.0",
  "description": "Client for OpenPetra running in the web browser",
  "main": "index.js",
  "type": "module",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6",
    "@popperjs/core": "^2.11.8",
    "axios": "^1.6.8",
    "bootstrap": "^5.3",
    "cypress": "^13.8.0",
    "i18next": "^10.6.0",
    "i18next-browser-languagedetector": "^2.2.4",
    "i18next-http-backend": "^2.5.1",
    "jquery": "^3.7",
    "uglify-js": "^3.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@types/i18next": "^13.0.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "css-loader": "^7",
    "postcss-loader": "^8",
    "sass": "^1.75.0",
    "sass-loader": "^16",
    "style-loader": "^4",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "build-debug": "webpack build --config webpack-debug.config.cjs --mode=development",
    "build-min": "webpack build --mode=production",
